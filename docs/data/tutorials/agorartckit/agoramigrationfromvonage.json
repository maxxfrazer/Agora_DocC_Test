(","        _ engine: AgoraRtcEngineKit, didJoinChannel channel: String,","        withUid uid: UInt, elapsed: Int","    ) {","        agoraEngine.startPreview()","        let videoCanvas = AgoraRtcVideoCanvas()","        self.localCanvas = videoCanvas","","        videoCanvas.uid = 0","        let localView = UIView()","        videoCanvas.renderMode = .hidden","        videoCanvas.view = localView","","        agoraEngine.setupLocalVideo(videoCanvas)","","        guard let localView = publisher.view else { return }","        let screenBounds = UIScreen.main.bounds","        localView.frame = CGRect(","            x: screenBounds.width - 150 - 20,","            y: screenBounds.height - 150 - 20,","            width: 150, height: 150","        )","        view.addSubview(localView)","    }","}"],"fileName":"VideoCallApp.swift","identifier":"vonagemigrationrtc-02-04-fullagora","type":"file","fileType":"swift","highlights":[]},"vonagemigrationrtc-02-02-subscribeagora":{"syntax":"swift","content":["import AgoraRtcKit","","class ViewController: UIViewController {","","\/\/    var subscriber: OTSubscriber?","    var remoteCanvas: AgoraRtcVideoCanvas?","","    ...","}","","\/\/ extension ViewController: OTSessionDelegate {","extension ViewController: AgoraRtcEngineDelegate {","\/\/    func session(_ session: OTSession, streamCreated stream: OTStream) {","    func rtcEngine(_ engine: AgoraRtcEngineKit, didJoinedOfUid uid: UInt, elapsed: Int) {","","\/\/        self.subscriber = OTSubscriber(stream: stream, delegate: self)","\/\/        guard let subscriber = subscriber else { return }","        let videoCanvas = AgoraRtcVideoCanvas()","        self.remoteCanvas = videoCanvas","        videoCanvas.uid = uid","        let remoteView = UIView()","        videoCanvas.view = remoteView","","\/\/        var error: OTError?","\/\/        session.subscribe(subscriber, error: &error)","\/\/        guard error == nil else { print(error!) return }","        agoraEngine.setupRemoteVideo(videoCanvas)","","        guard let remoteView = subscriber.view else { return }","        remoteView.frame = UIScreen.main.bounds","        self.view.insertSubview(remoteView, at: 0)","    }","}"],"fileName":"VideoCallApp.swift","identifier":"vonagemigrationrtc-02-02-subscribeagora","type":"file","fileType":"swift","highlights":[{"line":5},{"line":6},{"line":11},{"line":12},{"line":13},{"line":14},{"line":16},{"line":17},{"line":18},{"line":19},{"line":20},{"line":21},{"line":22},{"line":24},{"line":25},{"line":26},{"line":27},{"line":28}]},"vonagemigrationrtc-01-03-importrtc.png":{"alt":null,"type":"image","identifier":"vonagemigrationrtc-01-03-importrtc.png","variants":[{"url":"\/images\/vonagemigrationrtc-01-03-importrtc.png","traits":["1x","light"]}]},"https://swiftpackageindex.com/AgoraIO/AgoraRtcEngine_iOS":{"title":"AgoraRtcEngine_iOS on Swift Package Index","titleInlineContent":[{"type":"text","text":"AgoraRtcEngine_iOS on Swift Package Index"}],"type":"link","identifier":"https:\/\/swiftpackageindex.com\/AgoraIO\/AgoraRtcEngine_iOS","url":"https:\/\/swiftpackageindex.com\/AgoraIO\/AgoraRtcEngine_iOS"},"https://cocoapods.org/pods/AgoraRtcEngine_iOS":{"title":"CocoaPods: AgoraRtcEngine_iOS","titleInlineContent":[{"type":"text","text":"CocoaPods: AgoraRtcEngine_iOS"}],"type":"link","identifier":"https:\/\/cocoapods.org\/pods\/AgoraRtcEngine_iOS","url":"https:\/\/cocoapods.org\/pods\/AgoraRtcEngine_iOS"},"doc://AgoraRtcKit/tutorials/AgoraRtcKit/AgoraMigrationFromVonage#Check-Your-Understanding":{"title":"Check Your Understanding","titleInlineContent":[{"type":"text","text":"Check Your Understanding"}],"type":"link","identifier":"doc:\/\/AgoraRtcKit\/tutorials\/AgoraRtcKit\/AgoraMigrationFromVonage#Check-Your-Understanding","url":"\/tutorials\/agorartckit\/agoramigrationfromvonage#Check-Your-Understanding"},"vonage-agora-migration.png":{"alt":"Video UI Kit Banner Image","type":"image","identifier":"vonage-agora-migration.png","variants":[{"url":"\/images\/vo